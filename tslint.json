{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens": false,
        "curly": false,
        "no-console": false,
        "prefer-const": false,
        "quotemark": {
            "options": "single"
        },
        "trailing-comma": false,
        "unified-signatures": false,
        "no-namespace": false,
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}
